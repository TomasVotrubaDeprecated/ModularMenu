{
	"name": "zenify/modular-menu",
	"description": "Nette extension for building modular menus.",
	"license": "MIT",
	"authors": [
		{ "name": "Tomas Votruba", "homepage": "http://tomasvotruba.cz", "email": "tomas.vot@gmail.com" }
	],
	"require": {
		"php": ">=5.5",
		"nette/application": "~2.3",
		"nette/di": "~2.3"
	},
	"require-dev": {
		"nette/bootstrap": "~2.3",
		"nette/utils": "~2.3"
	},
	"autoload": {
		"psr-4": {
			"Zenify\\ModularMenu\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Zenify\\ModularMenu\\Tests\\": "tests"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0-dev"
		}
	}
}
